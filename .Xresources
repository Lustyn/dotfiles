Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight

! X colors.
! Generated by 'wal'
emacs*foreground:   #e78bc9
emacs*background:   #030205
URxvt*foreground:   #e78bc9
XTerm*foreground:   #e78bc9
UXTerm*foreground:  #e78bc9
URxvt*background:   [100]#030205
XTerm*background:   #030205
UXTerm*background:  #030205
URxvt*cursorColor:  #e78bc9
XTerm*cursorColor:  #e78bc9
UXTerm*cursorColor: #e78bc9
URxvt*borderColor:  [100]#030205

! Colors 0-15.
*.color0: #030205
*color0:  #030205
*.color1: #601B63
*color1:  #601B63
*.color2: #BA3F6A
*color2:  #BA3F6A
*.color3: #FAB139
*color3:  #FAB139
*.color4: #0D4D9B
*color4:  #0D4D9B
*.color5: #215AC6
*color5:  #215AC6
*.color6: #A30A9C
*color6:  #A30A9C
*.color7: #e78bc9
*color7:  #e78bc9
*.color8: #a1618c
*color8:  #a1618c
*.color9: #601B63
*color9:  #601B63
*.color10: #BA3F6A
*color10:  #BA3F6A
*.color11: #FAB139
*color11:  #FAB139
*.color12: #0D4D9B
*color12:  #0D4D9B
*.color13: #215AC6
*color13:  #215AC6
*.color14: #A30A9C
*color14:  #A30A9C
*.color15: #e78bc9
*color15:  #e78bc9

! Black color that will not be affected by bold highlighting.
*.color66: #030205
*color66:  #030205

! Xclock colors.
XClock*foreground: #e78bc9
XClock*background: #030205
XClock*majorColor:  rgba:e7/8b/c9/ff
XClock*minorColor:  rgba:e7/8b/c9/ff
XClock*hourColor:   rgba:e7/8b/c9/ff
XClock*minuteColor: rgba:e7/8b/c9/ff
XClock*secondColor: rgba:e7/8b/c9/ff

! Set depth to make transparency work.
URxvt*depth: 32
